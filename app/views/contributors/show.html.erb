<div class="page-header">
  <%= link_to contributors_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_contributor_path(@contributor), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show contributor</h1>
  <h3> Your Images </h3>
  <div class="row">
    <div class="col-sm-4">
      <%= link_to image_tag(@contributor.image.url(:large)), @contributor.image.url %>
    </div>
  </div>
</div>

<dl class="dl-horizontal">
</dl>
