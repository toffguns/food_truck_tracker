<div class="col-sm-1"></div>
<div class="col-sm-4" id="login-page2">
  <div id="form1">
  <h2>Log in as Contributor</h2>
  <%- if controller_name != 'sessions' %>
  <%= link_to "Log in", new_session_path(:contributor) %><br />
  <% end -%>

  <%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to "Didn't receive confirmation instructions?", new_confirmation_path(:contributor) %><br />
  <% end -%>

  <%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to "Didn't receive unlock instructions?", new_unlock_path(:contributor) %><br />
  <% end -%>

  <%- Contributor.omniauth_providers.each do |provider| %>
  <%= link_to image_tag("facebooklogin.png"), omniauth_authorize_path(:contributor, provider) %><br />
  <% end -%>
</div>
</div>

  <div class="col-sm-1"></div>
  <div class="col-sm-4" id="login-page">
    <div class="form1">
    <h2>Log in as a Food Truck Owner</h2>
    <%= bootstrap_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <%= f.email_field :email, autofocus: true %>
        <%= f.password_field :password, autocomplete: "off" %>
      <% if devise_mapping.rememberable? -%>
          <%= f.check_box :remember_me %>
      <% end -%>
        <%= f.submit "Log In" %>
    <% end %>
    <%= render "devise/shared/links" %>
      </div>
    </div>
